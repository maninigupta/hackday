<div class="row">
 <div class="col-md-6 col-md-offset-3">
  <%= form_for(@user) do |f| %>
   	<%= f.label :name %>
    <%= f.text_field :name, :placeholder => "name" %>

    <%= f.label :email %>
    <%= f.email_field :email, :placeholder => "email" %>

    <br>
    <br>

    <ziggeo ziggeo-width=320
       ziggeo-height=240>
		</ziggeo>

		<br>

    <%= f.submit "Sign me up", class: "btn btn-primary" %>
  <% end %>
 </div>
</div>